{
  "name": "gitbtn",
  "displayName": "gitbtn",
  "description": "quick open git repository by button",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:gitbtn.initBtn"
  ],
  "main": "./src/index.js",
  "contributes": {
    "commands": [
      {
        "command": "gitbtn.openRepository",
        "title": "openRepository"
      },
      {
        "command": "gitbtn.gitbtnInfo",
        "title": "gitbtn"
      },
      {
        "command": "gitbtn.initBtn",
        "title": "openRepositoryBtn"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "octicons": "^8.5.0",
    "simple-git": "^3.18.0"
  }
}
